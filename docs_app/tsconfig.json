{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "target": "ES2015",
    "allowJs": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "module": "es2015",
  },
  "exclude": [
    "content",
    "tools",
    "aio-builds-setup",
    "node_modules",
    "scripts"
  ],
  "paths": {
    "@ngx-lib/multiselect": [
      "../dist/multiselect"
    ],
    "@ngx-lib/multiselect/*": [
      "../dist/multiselect/*"
    ],
    "./app/": ["app/*"],
    "app/": ["app/*"],
  }
}