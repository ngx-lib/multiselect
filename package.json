{
  "name": "ngx-multiselect",
  "version": "0.0.4",
  "homepage": "https://ngx-lib.github.io/multiselect/",
  "scripts": {
    "ng": "ng",
    "start": "ng build multiselect && ng serve",
    "build-prod": "ng build --prod multiselect && ng build --prod --base-href /multiselect/",
    "build": "ng build",
    "onchange": "onchange",
    "multiselect-watch": "npx onchange \"./projects/multiselect/src/lib/**/*.*\" -- ng build multiselect",
    "test": "ng test --project=multiselect",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version",
    "build_lib": "ng build multiselect",
    "copy-license": "copy .\\LICENSE .\\dist\\multiselect",
    "copy-readme": "copy .\\README.md .\\dist\\multiselect",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/multiselect && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "prerelease": "npm run release -- --prerelease beta",
    "minor-release": "npm run release -- --release-as minor",
    "major-release": "npm run release -- --release-as major",
    "predeploy": "npm run build-prod",
    "deploy": "gh-pages -d dist/ngx-multiselect",
    "only-deploy": "gh-pages -d dist/ngx-multiselect"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.4",
    "@angular/cdk": "~7.1.1",
    "@angular/common": "7.0.4",
    "@angular/compiler": "7.0.4",
    "@angular/core": "7.0.4",
    "@angular/forms": "7.0.4",
    "@angular/http": "7.0.4",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "7.0.4",
    "@angular/platform-browser-dynamic": "7.0.4",
    "@angular/router": "7.0.4",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular-devkit/build-ng-packagr": "^0.7.5",
    "@angular/cli": "^7.0.6",
    "@angular/compiler-cli": "7.0.4",
    "@angular/language-service": "7.0.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "onchange": "^4.0.0",
    "protractor": "^5.4.0",
    "standard-version": "^4.4.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "^3.1.6"
  }
}
